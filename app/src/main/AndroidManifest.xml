            android:windowSoftInputMode="adjustPan" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".CollectionActivity" />
        <activity android:name=".MyPublishActivity" />
        <activity android:name=".MyUploadActivity" />
        <activity android:name=".MyDownloadActivity" />
        <activity android:name=".ContactUsActivity" />
        <activity android:name=".SettingActivity" />
        <activity android:name=".ExamActivity" />
        <activity android:name=".Postgraduate_examinationActivity" />
        <activity android:name=".ExpressActivity" />
        <activity android:name=".JobActivity" />
        <activity android:name=".LifeActivity" />
        <activity android:name=".NewsActivity" />
        <activity
            android:name=".SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MyOrderActivity" />
    </application>

</manifest>